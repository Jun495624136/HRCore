
@model Engage_resume_Model
@using System.Data

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>interview_register</title>
    <link href="~/CSS/table.css" rel="stylesheet" />
    <script src="~/JS/jquery-1.11.1.js"></script>
    <script src="~/JS/vue.js"></script>
    <script src="~/DatePicker/WdatePicker.js"></script>
    <script type="text/javascript">
        function search() {
            if (document.getElementById("date").value == "") {
                alert("请选择面试时间");
            } else {
                document.interviewForm.submit();
            }
        }
    </script>
</head>
<body>
    <form id="recruitAction!toList" asp-action="interview_register" method="post">
        <input type="hidden" name="operate" value="doAdopt">
        <!--	<input type="hidden" name="engageInterview.checkStatus" value=""/>
            <input type="hidden" name="engageInterview.checkComment" value=""/>
              -->
        <input type="hidden" name="engageInterview.einId" value="" />
        <input type="hidden" name="engageInterview.result" value="完成" />
        <input type="hidden" name="engageInterview.resumeId" value="542" />
        <input type="hidden" name="engageResume.resId" value="542" />
        <input type="hidden" name="engageInterview.interviewStatus" value="true" />
        <table width="100%">
            <tr>
                <td>

                    <font color="black">您正在做的业务是：人力资源--招聘管理--面试管理--面试结果登记  </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="面试登记" class="BUTTON_STYLE1"
                           onclick="search();">
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">

                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
           
                <tr>
                    <td class="TD_STYLE1" width="10%">
                        职位分类
                    </td>
                    <td width="10%" class="TD_STYLE2">
                        <input type="hidden" asp-for="@Model.Human_major_kind_id" />
                        <input type="text" asp-for="@Model.Human_major_kind_name" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td width="10%" class="TD_STYLE1">
                        职位名称
                    </td>
                    <td width="15%" class="TD_STYLE2">
                        <input type="hidden" asp-for="@Model.Human_major_id" />
                        <input type="text" asp-for="@Model.Human_major_name" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td width="10%" class="TD_STYLE1">
                        招聘类型
                    </td>
                    <td width="20%" class="TD_STYLE2" colspan="2">
                        <input type="text" asp-for="@Model.Engage_type" readonly="readonly" class="INPUT_STYLE2">
                    </td>

                    <td width="10%" rowspan="6" align="center">
                    </td>

                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        姓名
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_name" readonly="readonly" class="INPUT_STYLE2">

                    </td>
                    <td class="TD_STYLE1">
                        性别
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_sex" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        EMAIL
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_email" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        电话号码
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_telephone" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        家庭电话
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_homephone" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        手机
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_mobilephone" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        住址
                    </td>
                    <td colspan="3" class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_address" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        邮编
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_postcode" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        国籍
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_nationality" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        出生地
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_birthplace" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        生日
                    </td>
                    <td class="TD_STYLE2" colspan="2">
                        <input type="text" asp-for="@Model.Human_birthday" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>

                <tr>
                    <td width="11%" class="TD_STYLE1">
                        民族
                    </td>
                    <td class="TD_STYLE2" width="14%">
                        <input type="text" asp-for="@Model.Human_race" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        宗教信仰
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_religion" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        政治面貌
                    </td>
                    <td class="TD_STYLE2" colspan="2">
                        <input type="text" asp-for="@Model.Human_party" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>
                <tr>

                    <td class="TD_STYLE1">
                        身份证号码
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_idcard" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        年龄
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_age" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        毕业学校
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_college" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1" width="10%">
                        学历
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_educated_degree" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        教育年限
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_educated_years" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        学历专业
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_educated_major" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        薪酬要求
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Demand_salary_standard" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        注册时间
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Regist_time" readonly="readonly" class="INPUT_STYLE2">

                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        特长
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_specility" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        爱好
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Human_hobby" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        <span style="font-size: 10.5pt; font-family: 宋体;"></span>
                        推荐人
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Checker" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        <span style="font-size: 10.5pt; font-family: 宋体;"></span>
                        推荐时间
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" asp-for="@Model.Check_time" readonly="readonly" class="INPUT_STYLE2">

                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        个人履历
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        <textarea asp-for="@Model.Human_history_records" rows="4"
                                  class="TEXTAREA_STYLE1" readonly="readonly"> 
						</textarea>
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        备注
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        <textarea asp-for="@Model.Remark" rows="4" class="TEXTAREA_STYLE1" readonly="readonly"> 
						</textarea>
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        推荐意见
                    </td>
                    <td class="TD_STYLE2" colspan="7">
                        <textarea asp-for="@Model.Recomandation" rows="4" class="TEXTAREA_STYLE1" readonly="readonly">					</textarea>
                    </td>
                </tr>

            

                <tr>
                    <td class="TD_STYLE1">
                        面试次数
                    </td>
                    <td class="TD_STYLE2">
                        @*@Html.TextBoxFor(e => e.Engage_Interview_Model.Interview_amount,new { @class = "INPUT_STYLE2", Value = ViewBag.Ms })*@
                        <input type="text" asp-for="@Model.Engage_Interview_Model.Interview_amount" class="INPUT_STYLE2" readonly="readonly" value="@ViewBag.Ms" />
                    </td>
                    <td class="TD_STYLE1">
                        形象评价
                    </td>
                    <td class="TD_STYLE2">
                        @Html.DropDownListFor(e => e.Engage_Interview_Model.Image_degree, new SelectList(ViewBag.hard_value, "Value", "Text"), new { @class = "SELECT_STYLE1", })
                    </td>
                    <td class="TD_STYLE1">
                        口才评价
                    </td>
                    <td class="TD_STYLE2">
                        @Html.DropDownListFor(e => e.Engage_Interview_Model.Native_language_degree, new SelectList(ViewBag.hard_value, "Value", "Text"), new { @class = "SELECT_STYLE1", })
                    </td>
                    <td class="TD_STYLE1">
                        外语口语能力
                    </td>
                    <td class="TD_STYLE1">
                        @Html.DropDownListFor(e => e.Engage_Interview_Model.Foreign_language_degree, new SelectList(ViewBag.hard_value, "Value", "Text"), new { @class = "SELECT_STYLE1", })
                    </td>
                </tr>
            <tr>
                <td class="TD_STYLE1">
                    应变能力
                </td>
                <td class="TD_STYLE2">
                    @Html.DropDownListFor(e => e.Engage_Interview_Model.Response_speed_degree, new SelectList(ViewBag.hard_value, "Value", "Text"), new { @class = "SELECT_STYLE1", })
                </td>
                <td class="TD_STYLE1">
                    EQ
                </td>
                <td class="TD_STYLE2">
                    @Html.DropDownListFor(e => e.Engage_Interview_Model.EQ_degree, new SelectList(ViewBag.hard_value, "Value", "Text"), new { @class = "SELECT_STYLE1", })
                </td>
                <td class="TD_STYLE1">
                    IQ
                </td>
                <td class="TD_STYLE2">
                    @Html.DropDownListFor(e => e.Engage_Interview_Model.IQ_degree, new SelectList(ViewBag.hard_value, "Value", "Text"), new { @class = "SELECT_STYLE1", })
                </td>
                <td class="TD_STYLE1">
                    综合素质
                </td>
                <td class="TD_STYLE1">
                    @Html.DropDownListFor(e => e.Engage_Interview_Model.Multi_quality_degree, new SelectList(ViewBag.hard_value, "Value", "Text"), new { @class = "SELECT_STYLE1", })
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    面试人
                </td>
                <td class="TD_STYLE2">
                    @Html.DropDownListFor(e => e.Engage_Interview_Model.Register, ViewData["sa"] as IEnumerable<SelectListItem>, new { @class = "SELECT_STYLE1" })
                    @*<input type="text" name="engageInterview.register" value="zhangsan" class="INPUT_STYLE2" />*@
                </td>
                <td class="TD_STYLE1">
                    面试时间
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Engage_Interview_Model.Registe_time" name="engageInterview.registeTime" onclick="WdatePicker()")"
                           class="INPUT_STYLE2" id="date">


                </td>
                <td class="TD_STYLE1">
                    &nbsp;
                </td>
                <td class="TD_STYLE2">
                    &nbsp;
                </td>
                <td class="TD_STYLE1">
                    &nbsp;
                </td>
                <td class="TD_STYLE2">
                    &nbsp;
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    面试评价
                </td>
                <td class="TD_STYLE2" colspan="7">
                    @Html.TextAreaFor(e => e.Engage_Interview_Model.Interview_comment, new { @class = "TEXTAREA_STYLE1" })
                </td>
            </tr>
            
        </table>
    </form>
</body>
</html>
