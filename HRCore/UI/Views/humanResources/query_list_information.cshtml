@model Human_file_Model

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>query_list_information</title>
    <link href="~/CSS/table.css" rel="stylesheet" />
</head>
<body>
    <form name="humanfileForm" method="post" action="/hr/humanfile.do">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">您正在做的业务是：人力资源--人力资源档案管理--人力资源档案查看 </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back()">
                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=3 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td class="TD_STYLE1" width="10%">
                    档案编号
                </td>
                <td colspan="6" class="TD_STYLE2">
                    <input type="hidden" asp-for="@Model.Huf_id" />
                    <input type="text" asp-for="@Model.Human_id" readonly="readonly" class="INPUT_STYLE2" />
                </td>
                <td rowspan="6" width="13%" style="text-align: center;">
                    <img src="@Model.Human_picture" style="width:120px;height:150px;" />
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1" width="10%">
                    I级机构
                </td>
                <td width="13%" class="TD_STYLE2">
                    <input type="text" asp-for="@Model.First_kind_name" readonly="readonly" class="INPUT_STYLE2" />
                </td>
                <td width="10%" class="TD_STYLE1">
                    II级机构
                </td>
                <td width="13%" class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Second_kind_name" readonly="readonly" class="INPUT_STYLE2" />
                </td>
                <td width="10%" class="TD_STYLE1">
                    III级机构
                </td>
                <td class="TD_STYLE2" colspan="2" width="2%">
                    <input type="text" asp-for="@Model.Third_kind_name" readonly="readonly" class="INPUT_STYLE2" />
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    职位分类
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_major_kind_name" readonly="readonly" class="INPUT_STYLE2" />
                </td>
                <td class="TD_STYLE1">
                    职位名称
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Hunma_major_name" readonly="readonly" class="INPUT_STYLE2" />
                </td>
                <td class="TD_STYLE1">
                    职称
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_pro_designation" readonly="readonly" class="INPUT_STYLE2" />
                  
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    姓名
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_name"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    性别
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_sex" readonly="readonly" class="INPUT_STYLE2" />
                </td>
                <td class="TD_STYLE1">
                    EMAIL
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_email"
                           class="INPUT_STYLE2" id="humanEmail">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    电话
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_telephone" class="INPUT_STYLE2" />
                </td>
                <td class="TD_STYLE1">
                    QQ
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_qq"
                           class="INPUT_STYLE2" id="humanQq">
                </td>
                <td class="TD_STYLE1">
                    手机
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_mobilephone"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    住址
                </td>
                <td colspan="3" class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_address"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    邮编
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_postcode"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    国籍
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_nationality" readonly="readonly" class="INPUT_STYLE2" />

                </td>
                <td class="TD_STYLE1">
                    出生地
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_birthplace"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    生日
                </td>
                <td width="13%" class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_birthday" readonly="readonly"
                           class="INPUT_STYLE2" onclick="WdatePicker()">
                </td>
                <td width="10%" class="TD_STYLE1">
                    民族
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_race" readonly="readonly" class="INPUT_STYLE2" />
                 
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    宗教信仰
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_religion" readonly="readonly" class="INPUT_STYLE2" />
                  
                </td>
                <td class="TD_STYLE1">
                    政治面貌
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_party" readonly="readonly" class="INPUT_STYLE2" />
                  
                </td>
                <td class="TD_STYLE1">
                    身份证号码
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_id_card"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    社会保障号码
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_society_security_id"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    年龄
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_age"
                           class="INPUT_STYLE2" id="humanAge">
                </td>
                <td class="TD_STYLE1">
                    学历
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_educated_degree" readonly="readonly" class="INPUT_STYLE2" />
                  
                </td>
                <td class="TD_STYLE1">
                    教育年限
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_educated_years" readonly="readonly" class="INPUT_STYLE2" />
                   
                </td>
                <td class="TD_STYLE1">
                    学历专业
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_educated_major" readonly="readonly" class="INPUT_STYLE2" />
                   
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    薪酬标准
                </td>
                <td class="TD_STYLE2">
                    <input type="hidden" asp-for="@Model.Salary_standard_id" />
                    <select asp-for="@Model.Salary_standard_name" class="SELECT_STYLE1">
                        <option>总裁级别</option>
                        <option>经理级别</option>
                        <option>主管级别</option>
                        <option>职员级别</option>
                    </select>
                </td>
                <td class="TD_STYLE1">
                    开户行
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_bank"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    帐号
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_account"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    复核人
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Checker" readonly="readonly" class="INPUT_STYLE2" />
                   
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    复核时间
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Check_time"
                           id="nowTime" readonly="readonly"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    特长
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_speciality" readonly="readonly" class="INPUT_STYLE2" />
                   
                </td>
                <td class="TD_STYLE1">
                    爱好
                </td>
                <td class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_hobby" readonly="readonly" class="INPUT_STYLE2" />
                   
                </td>
                <td class="TD_STYLE1">
                    &nbsp;
                </td>
                <td class="TD_STYLE2">
                    &nbsp;
                </td>
            </tr>
            @*<tr>
                <td class="TD_STYLE1">
                    档案附件
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <a href="#"></a>
                </td>
            </tr>*@
            <tr>
                <td class="TD_STYLE1">
                    个人履历
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea asp-for="@Model.Human_histroy_records" rows="4"
                              class="TEXTAREA_STYLE1"></textarea>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    家庭关系信息
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea asp-for="@Model.Human_family_membership" rows="4"
                              class="TEXTAREA_STYLE1"></textarea>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    备注
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea asp-for="@Model.Remark" rows="4" class="TEXTAREA_STYLE1" readonly="readonly"></textarea>
                </td>
            </tr>
        </table>
    </form>
</body>
</html>
