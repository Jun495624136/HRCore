
@model Human_file_Model

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>delete_keywords</title>
    <link href="~/CSS/table.css" rel="stylesheet" />
    <script src="~/JS/jquery-1.11.1.js"></script>
    <script src="~/JS/vue.js"></script>
</head>
<body>
    <form asp-action="delete_keywords" asp-controller="delete" method="post">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">您正在做的业务是：人力资源--人力资源档案管理--人力资源档案删除查询</font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="开始" class="BUTTON_STYLE1" />
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">
                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr class="TR_STYLE1">
                <td width="16%" class="TD_STYLE1">
                    请输入关键字
                </td>
                <td width="84%" class="TD_STYLE2">
                    <input type="text" asp-for="@Model.Human_name" class="INPUT_STYLE1">
                </td>
            </tr>
        </table>
    </form>
</body>
</html>
